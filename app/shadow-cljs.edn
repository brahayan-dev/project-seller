{:source-paths
 ["source"]

 :dependencies
 [[reagent "1.0.0"]
  [cljs-ajax "0.8.1"]
  [metosin/reitit "0.5.12"]]

 :dev-http {1234 "public"}
 :builds
 {:app {:target :browser
        :output-dir "public/assets"
        :modules {:main {:init-fn core/main}}
        :release {:compiler-options {:optimizations :advanced :output-feature-set :es5}}
        :dev {:compiler-options {:output-feature-set :es6}}}
  :test {:target    :node-test
         :output-to ".shadow-cljs/__test__/node-tests.js"
         :ns-regexp "-test$"
         :autorun   true}}}
